<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="500.0" prefWidth="645.0">
    <children>
        <Pane prefHeight="375.0" prefWidth="447.0">
            <children>
                <TextField fx:id="chooseFPGATextField" layoutX="185.0" layoutY="51.0"
                           promptText="choose fpga" text="choose fpga"/>
                <ButtonBar fx:id="chooseCryptoModuleButtonBar" layoutY="114.0"
                           prefHeight="81.0"
                           prefWidth="446.0">
                    <buttons>
                        <Button fx:id="initAESModule" minHeight="60.0"
                                mnemonicParsing="false"
                                text="Init AES" wrapText="true"/>
                        <Button fx:id="initHashFunctionModule" minHeight="60.0"
                                mnemonicParsing="false" text="Init Hash Function"
                                wrapText="true"/>
                        <Button fx:id="initEdDSAModule" minHeight="60.0"
                                mnemonicParsing="false"
                                text="Init EdDSA" wrapText="true"/>
                        <Button fx:id="initDilithiumModule" minHeight="60.0"
                                mnemonicParsing="false" text="Init Dilithium"
                                wrapText="true"/>
                        <Button fx:id="initCustomModule" minHeight="60.0"
                                mnemonicParsing="false" text="Init Custom Module"
                                wrapText="true"/>
                    </buttons>
                </ButtonBar>
                <Button fx:id="chooseFpgaButton" layoutX="30.0" layoutY="51.0"
                        mnemonicParsing="false" text="Choose FPGA"/>
            </children>
        </Pane>
    </children>
</BorderPane>