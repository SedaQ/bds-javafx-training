<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="500.0" prefWidth="645.0" xmlns="http://javafx.com/javafx/10.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="cz.vutbr.feec.fpga.controllers.EncryptionDecryptionContentController">
    <children>
        <BorderPane>
            <children>
                <Pane prefHeight="375.0" prefWidth="447.0">
                    <children>
                        <TextField layoutX="185.0" layoutY="51.0" promptText="choose fpga 2" text="choose fpga"/>
                    </children>
                </Pane>
            </children>
        </BorderPane>
    </children>
    <center>
        <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <TextArea fx:id="textAreaEncryptDecryptInput" layoutX="84.0" layoutY="109.0" prefHeight="177.0"
                          prefWidth="477.0" promptText="Drag and Drop or Paste Data Here"/>
                <TextArea fx:id="textAreaEncryptDecryptOutput" layoutX="85.0" layoutY="353.0" prefHeight="133.0"
                          prefWidth="476.0" promptText="Output from FPGA"/>
                <Button fx:id="takeInputDataAndEncrypt" layoutX="100.0" layoutY="306.0" mnemonicParsing="false"
                        prefHeight="25.0" prefWidth="191.0" text="Take input data and encrypt"/>
                <Button fx:id="takeInputDataAndDecrypt" layoutX="361.0" layoutY="306.0" mnemonicParsing="false"
                        prefHeight="25.0" prefWidth="186.0" text="Take input data and decrypt"/>
            </children>
        </Pane>
    </center>
</BorderPane>
